{
  "name": "JavascriptCoronaProject",
  "version": "1.0.0",
  "description": "C'est un projet pour valider les comp√©tences C3N3 (C2N3).",
  "main": "index.js",
  "scripts": {
    "watch-sass": "node-sass Starter-file/sass/main.scss Starter-file/css/style.css --watch",
    "compile-sass": "node-sass Starter-file/sass/main.scss Starter-file/css/style.comp.css",
    "concat-css": "concat -o Starter-file/css/style.concat.css Starter-file/css/additional.css Starter-file/css/style.comp.css",
    "prefix-css": "postcss --use autoprefixer -b 'last 5 versions' Starter-file/css/style.concat.css -o  Starter-file/css/style.prefix.css",
    "compress-css": "node-sass Starter-file/css/style.prefix.css Starter-file/css/style.css --output-style compressed",
    "build-css": "npm-run-all compile-sass concat-css prefix-css compress-css"
  },
